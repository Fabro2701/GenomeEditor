<CODE> :- [
		   Header(name=Str(text="code")),
		   Children(e=<LINE>),
		   Floor()
		  ],
		  [
		   Header(name=Str(text="code")),
		   Children(e=<CODE>),
		   Children(e=<LINE>),
		   Floor()
		  ].
<LINE> :- [
 		   Header(name=Str(text="line")),
 		   Children(e=<IF>),
 		   Floor()
		  ],
		  [
		   Header(name=Str(text="return"))
		  ].
<IF> :- [
		 Header(name=Str(text="if"),right=<COND>),
		 Children(),
		 Floor()
		].

<COND> :- [
		   Header(right=<OBS>,right=Str(text="+"),right=<OBS>)
		  ].
<OBS> :- [Header(name=Str(text="ae"))],[Header(name=Str(text="obs2"))],[Header(name=Str(text="qqqqqqq"))].