<CODE> :- [
		   Header(name=Str(text="code")),
		   Children(e=<IF>),
		   Floor()
		  ].
<IF> :- [
		 Header(name=Str(text="if"),right=<COND>),
		 Children(),
		 Floor()
		].
<COND> :- [
		   Header(right=<OBS>,right=Str(text="+"),right=<OBS>)
		  ].
<OBS> :- [Str(text="ae")],[Str(text="obs2")],[Str(text="qqqqqqq")].